{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "proseWrap": "always",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-packagejson", "prettier-plugin-svelte"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
